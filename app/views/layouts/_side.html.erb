<ul class="nav nav-pills nav-stacked"> 
	<% unless @two_level_menus.nil? %>
		<% @two_level_menus.each do |menu| %>
			<li>
				<%= link_to "#{menu.url}?menu_id=#{menu.id}", remote: true, class: "two_level", id: "/#{menu.controller}" do %>
					<span class="glyphicon glyphicon-<%= menu.icon %>"></span> <%= menu.name %>	
				<% end %>
			</li>
		<% end %>
	<% end %>	
</ul>